{
    "name": "Speed Control Extension",
    "version": "2.0",
    "description": "Control video playback speed on YouTube with simple buttons.",
    "manifest_version": 3,
    "permissions": ["activeTab", "storage", "webNavigation"],
    "options_page": "options.html",
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/watch*"],
        "css": ["styles.css"],
        "js": ["dist/bundle.js"]
      }
    ]
  }
  